            var kfi = 1;

            function koordinateFormat() {

                if (kfi == 1) {

                    document.getElementById("kf0").innerHTML = "45°&nbsp;33,7998'N&nbsp;/&nbsp;14°&nbsp;16,6578'E";

                        kfi = 2;

                    }

                    else if (kfi == 2) {

                        document.getElementById("kf0").innerHTML = "45°&nbsp;33'&nbsp;47,99''N&nbsp;/&nbsp;14°&nbsp;16'&nbsp;39,47''E";

                        kfi = 3;

                    }

                    else if (kfi == 3) {

                        document.getElementById("kf0").innerHTML = "45.56333°N&nbsp;/&nbsp;14.27763°E";

                    kfi = 4;

                }

                else if (kfi == 4) {

                    document.getElementById("kf0").innerHTML = "45,56333°N,&nbsp;14,27763°E";

                        kfi = 1;

                    }

    }



            function kopiraj() {

                var tempInput = document.createElement("textarea");

                tempInput.value = document.getElementById("kf0").innerHTML.replace(/&nbsp;/g, " ");

                document.body.appendChild(tempInput);

                tempInput.select();

                tempInput.setSelectionRange(0, 99999);

                document.execCommand("copy");

                document.body.removeChild(tempInput);

                tooltip("Kopirano v odložišče...");



            }



    

</script>

        

